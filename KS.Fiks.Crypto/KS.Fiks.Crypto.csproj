<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>KS FIKS Common Crypto</Title>
        <Authors>KS</Authors>
        <Copyright>KS</Copyright>
        <PackageProjectUrl>https://github.com/ks-no/kryptering-dotnet</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/ks-no/kryptering-dotnet/raw/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>git@github.com:ks-no/kryptering-dotnet.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>FIKS</PackageTags>
        <VersionPrefix>1.0.0</VersionPrefix>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)'=='Debug'">
        <DebugType>full</DebugType>
        <DebugSymbols>True</DebugSymbols>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Portable.BouncyCastle" Version="1.8.5"/>
        <PackageReference Include="KS.Fiks.QA" Version="0.9.9-dev-62" PrivateAssets="All"/>
    </ItemGroup>

</Project>
